{
    "$id": "https://w3id.org/fuel/schemas/1.0.0/diagnostics/thermocouple",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "description": "Metadata to describe thermocouple diagnostic",
    "type": "object",
    "required": [ "insertRequired" ],
    "properties": {
        "deviceInformation": {
            "type": "object",
            "properties": {
                "required": ["insertRequired"],
                "attachmentType": {
                    "type": "string",
                    "description": "[-]",
                    "example": "spot weld with inert gas shield"
                },
                "tcType": {
                    "type": "string",
                    "description": "[-]",
                    "example": "K"
                }
            },
            "deviceSetup": {
                "required": ["insertRequired"],
                "type": "object",
                "properties": {
                    "dataChannel": {
                        "type": "str",
                        "descripiton": "[-]"
                    },
                    "location": {
                        "type": "str",
                        "descripiton": "[-]"
                    }
                }
            }
        }
    }
}
