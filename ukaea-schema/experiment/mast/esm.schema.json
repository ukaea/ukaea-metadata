            },
            "dimensions": {
                "type": "object",
                "properties": {}
            }
        },
        "dr_sep_in": {
            "type": "object",
            "description": "drsep - the distance between the two separatrices on the inboard midplane (taken at Z=Z0) of the plasma.",
            "units": {
                "type": "string",
                "const": "m"
